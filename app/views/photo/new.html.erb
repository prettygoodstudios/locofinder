<%= form_for(@photo, url: photo_index_path) do |f| %>
  <div class="form-group">
    <%= f.label :caption %>
    <%= f.text_field :caption %>
  </div>
  <div class="form-group">
    <%= f.label :img_url %>
    <%= f.file_field :img_url %>
  </div>
  <%= f.hidden_field :location, value: @location.id %>
  <%= f.hidden_field :user, value: current_user.id %>
  <%= f.submit %>
<% end %>
